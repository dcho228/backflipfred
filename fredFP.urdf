<?xml version="1.0"?>

<robot name="Fred">

  <link name="base_link">
    <inertial>
      <origin xyz="0 0 2" rpy="0 0 0" />
      <mass value="5" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="2" />
    </inertial>
    <visual>
      <origin xyz="0 0 2" rpy="0 0 0" />
      <geometry>
         <box size="3.0 1 1" />
      </geometry>
      <material name="blue">
	       <color rgba="0 0 1 1" />
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="3.0 1 0.5" />
      </geometry>
      <origin xyz="0 0 2" rpy="0 0 0" />
    </collision>
  </link>
  
  <link name="camera">
     <visual>
         <geometry>
             <cylinder length="0.1" radius="0.25"/>
         </geometry>
         <material name="white">
             <color rgba="1 1 1 1" />
         </material>
     </visual>
     </link>    
     
     <joint name="base_to_camera" type="fixed">
        <parent link="base_link"/>
        <child link="camera"/>
        <origin rpy="0 1.57 0" xyz="1.55 0.0 2.05"/>
     </joint>

